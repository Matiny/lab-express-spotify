<div class="container-box">
    {{#if this.images.0.url}}
    <img class="image-result" src="{{this.images.0.url}}">
    {{else}}
    <img src="https://developer.spotify.com/assets/branding-guidelines/icon1@2x.png">
    {{/if}}

    <div class="description">
        <p>{{ this.name }}</p>
        <button>
            <a href="/tracks/{{ this.id }}">View Tracks</a>
        </button>
    </div>
</div>